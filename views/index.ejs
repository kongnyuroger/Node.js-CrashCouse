<html lang="en">
    <%- include('./partials/head') %>
<body>
   <%- include('./partials/nav') %>
    <div class="blogs content">
        <h2>All blogs</h2>
        <% if (blogs.length > 0) {%>
            <% blogs.forEach(blog => {%>
                <h3 class="title"><%= blog.title %></h3>
                <h2 class="snippet"><%= blog.snippet %></h2>
            <%})%>
            <% } else {%>
                <p>the are no blogs to display</p>
            <%}%>
    </div>
    <%- include('./partials/footer') %>
</body>
</html>